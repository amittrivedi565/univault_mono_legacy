<!DOCTYPE html>
<html lang="en">
<%- include('includes/head') %>
<body class="branch-body">
    <%- include('includes/nav') %>
    <div class="options-wrapper">
        <% uniQuery.forEach(uni => { %>
                <% uni.course.forEach(course => { %>
                    <!-- LowerCase Logic for Branch , Course Shortname -->
                    <%  
                        var c = course.shortname
                        var lowerC = c.toLowerCase();
                    %>
                    <div class="option-container">
                        <div class="option-container-top">
                            <a href="/<%=uni.shortname%>/<%=lowerC%>/">#&nbsp;<%= course.name %></a>
                            <a><%= course.shortname %></a>
                        </div>
                    </div>
                <% }) %>
        <% }) %>
    </div>
    <%- include('includes/script') %>
</body>
</html>