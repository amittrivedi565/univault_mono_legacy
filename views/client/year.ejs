<!DOCTYPE html>
<html lang="en">
<%- include('includes/head') %>
<body class="branch-body">
    <%- include('includes/nav') %>
    <div class="options-wrapper">
        <% uniQuery.forEach(uni => { %>
            <% uni.branch.forEach(branch => { %>
                <% branch.course.forEach(course => { %>
                    <% course.years.forEach(year => { %>
                        <div class="option-container">
                            <div class="option-container-top">
                                <a href="/<%= uni.name %>/<%= branch.shortname %>/<%= course.code %>/<%= year.name %>">#&nbsp;<%=year.name%></a>
                                <a><%= branch.shortname %>&nbsp;<%= course.name %></a>
                            </div>
                        </div>
                    <% }) %>
                <% }) %>
            <% }) %>
        <% }) %>
    <%- include('includes/script') %>
</body>
</html>